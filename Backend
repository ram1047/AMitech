      const phoneNumber = '918007931310';
      const message = 'Hello! I am interested in your cybersecurity training programs. Please share details about red team labs and certifications.';
      window.open(`https://wa.me/${phoneNumber}?text=${encodeURIComponent(message)}`, '_blank');
